{
    "manifestVersion": 1,
    "id": "btr-apktool",
    "version": "0.1.26",
    "name": "ApkTool Build and Release Task",
    "scopes": [
        "vso.build",
        "vso.release"
    ],
    "description": "This task wraps the ApkTool command line tool making it easy to invoke as part of a build/release pipeline.",
    "publisher": "BuiltToRoam",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "build, release, packaging, android, apk, apktool"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://github.com/builttoroam/apktool-azure-devops-ext"
        },
        "getstarted": {
            "uri": "https://github.com/builttoroam/apktool-azure-devops-ext"
        },
        "learn": {
            "uri": "https://github.com/builttoroam/apktool-azure-devops-ext"
        },
        "support": {
            "uri": "https://github.com/builttoroam/apktool-azure-devops-ext"
        },
        "repository": {
            "uri": "https://github.com/builttoroam/apktool-azure-devops-ext"
        },
        "issues": {
            "uri": "https://github.com/builttoroam/apktool-azure-devops-ext"
        }
    },
    "branding": {
        "color": "rgb(32, 41, 45)",
        "theme": "dark"
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "apktool",
            "addressable": true
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "contributions": [
        {
            "id": "ApkTool",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ApkTool"
            }
        }
    ]
}